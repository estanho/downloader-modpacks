{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "sn-modpacks",
  "version": "0.1.0",
  "identifier": "com.sn-modpacks.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Sn ModPacks",
        "width": 600,
        "height": 400,
        "resizable": false,
        "maximizable": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": []
  }
}
